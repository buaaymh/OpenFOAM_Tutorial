3
(
    movingWall
    {
        type            patch;
        nFaces          10;
        startFace       180;
    }
    fixedWalls
    {
        type            wall;
        inGroups        List<word> 1(wall);
        nFaces          30;
        startFace       190;
    }
    frontAndBack
    {
        type            empty;
        inGroups        List<word> 1(empty);
        nFaces          200;
        startFace       220;
    }
)

