FoamFile
{
    format      ascii;
    class       polyBoundaryMesh;
    location    "constant/polyMesh";
    object      boundary;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

4
(
    movingWall
    {
        type            wall;
        inGroups        List<word> 1(wall);
        nFaces          3;
        startFace       16;
    }
    fixedWalls
    {
        type            wall;
        inGroups        List<word> 1(wall);
        nFaces          7;
        startFace       19;
    }
    frontAndBack
    {
        type            empty;
        inGroups        List<word> 1(empty);
        nFaces          24;
        startFace       26;
    }
    procBoundary0to1
    {
        type            processor;
        inGroups        List<word> 1(processor);
        nFaces          6;
        startFace       50;
        matchTolerance  0.0001;
        myProcNo        0;
        neighbProcNo    1;
    }
)

