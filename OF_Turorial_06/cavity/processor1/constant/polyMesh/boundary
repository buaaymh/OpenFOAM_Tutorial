FoamFile
{
    format      ascii;
    class       polyBoundaryMesh;
    location    "constant/polyMesh";
    object      boundary;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

4
(
    movingWall
    {
        type            wall;
        inGroups        List<word> 1(wall);
        nFaces          2;
        startFace       18;
    }
    fixedWalls
    {
        type            wall;
        inGroups        List<word> 1(wall);
        nFaces          8;
        startFace       20;
    }
    frontAndBack
    {
        type            empty;
        inGroups        List<word> 1(empty);
        nFaces          26;
        startFace       28;
    }
    procBoundary1to0
    {
        type            processor;
        inGroups        List<word> 1(processor);
        nFaces          6;
        startFace       54;
        matchTolerance  0.0001;
        myProcNo        1;
        neighbProcNo    0;
    }
)

